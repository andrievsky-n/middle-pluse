{
    "extends": "airbnb-base",
    "globals": {
        "$": true,
        "document": true,
        "window": true
    },
    "rules": {
        "func-names": 0,
        "indent": [2, 4, {"SwitchCase": 1}],
        "linebreak-style": 0,
        "no-unused-vars": 0,
        "padded-blocks": 0,
        "spaced-comment": 1,
        "comma-dangle": 0,
        "prefer-const": 1,
        "space-before-function-paren": 1,
        "no-trailing-spaces": 1,
        "no-var": 1,
        "prefer-arrow-callback": 1,

        "import/no-unresolved": 0,
        "import/extensions": 0,
        "import/no-extraneous-dependencies": 0,
        "no-useless-escape": 1,
        "prefer-template": 1,
        "object-shorthand": 1,
        "space-in-parens": 1,
        "consistent-return": 1,
        "no-param-reassign": 1,
        "vars-on-top": 1,
        "no-plusplus": 1,
        "no-else-return": 1,
        "max-len": 1,
        "eqeqeq": 1,
        "no-restricted-syntax": 1,
        "guard-for-in": 1,
        "block-scoped-var": 1,
    }
}
